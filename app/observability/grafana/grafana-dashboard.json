{
    "title": "FastAPI SRE Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "Request Latency (95th Percentile)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(app_request_latency_seconds_bucket[5m]))"
          }
        ]
      },
      {
        "type": "stat",
        "title": "Characters Processed",
        "targets": [
          {
            "expr": "app_characters_processed_total"
          }
        ]
      },
      {
        "type": "stat",
        "title": "Cache Hit Ratio",
        "targets": [
          {
            "expr": "rate(app_cache_hits_total[5m]) / rate(http_requests_total[5m])"
          }
        ]
      }
    ]
  }
  